class Solution:
    def minAddToMakeValid(self, s: str) -> int:
        # ())) == 2
        # (())))

        # ())((. 
        # (()((. 
        r = 0
        l = 0
        for x in s:
            if  x == '(':
               l +=1
            else:
                if (l!= 0):
                    l-=1
                else:
                    r+=1
        return l+r
s=  "())"
s1 =  "((("
sol = Solution()
res = sol.
print(s)
res1 = sol.minAddToMakeValid(s1)
print(s1) 