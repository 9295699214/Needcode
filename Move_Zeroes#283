class Solution:
    def moveZeroes(self, nums: list[int]) -> None:
        """
        Do not return anything, modify nums in-place instead.
        """
        i=0
        while(i<len(nums)):
            if nums[i]==0:
                nums.append(0)
                nums.remove(0)
            i+=1
nums = [0,1,0,3,12]
nums1= [0]
sol = Solution()
sol.moveZeroes(nums)
print(nums)
sol.moveZeroes(nums1)
print(nums1)